{
  "name": "@wal-li/ui",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate:code && vite --host 0.0.0.0",
    "generate:code": "node ./scripts/generate-code.mjs",
    "build:lib": "npm run generate:code && rollup -c && node ./scripts/postbuild.mjs",
    "build:example": "npm run generate:code && vite build"
  },
  "exports": {
    "./*": {
      "node": {
        "import": "./*/index.mjs",
        "require": "./*/index.cjs"
      },
      "default": "./*/index.mjs"
    },
    "./theme.css": "./theme.css"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "lorem-ipsum": "^2.0.8",
    "postcss": "^8.4.45",
    "rollup": "^4.21.2",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vue": "^3.5.2",
    "vue-router": "^4.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wal-li/framework.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
